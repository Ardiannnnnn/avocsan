{
  "expo": {
    "name": "AvoScan",
    "slug": "avocado",
    "version": "1.0.0",
    "scheme": "acme",
    "userInterfaceStyle": "automatic",
    "orientation": "default",
    "splash": {
      "image": "./assets/splash_new.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ardi_expo.avocado",
      "infoPlist": {
        "NSCameraUsageDescription": "Aplikasi membutuhkan akses kamera untuk mendeteksi alpukat."
      }
    },
    "android": {
      "package": "com.ardi_expo.avocado",
      "minSdkVersion": 26,
      "adaptiveIcon": {
        "foregroundImage": "./assets/logobaru.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": ["android.permission.CAMERA"]
    },

    "web": {
      "output": "static",
      "bundler": "metro"
    },
    "assetBundlePatterns": ["**/*", "assets/**/*", "assets/models/*"],
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://n"
        }
      ],
      [
        "react-native-vision-camera",
        {
          "cameraPermissionText": "Camera access for avocado scanning",
          "enableMicrophonePermission": false,
          "enableCodeScanner": false,
          "enableFrameProcessors": true
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "minSdkVersion": 26
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": "https://n"
      },
      "eas": {
        "projectId": "a47a7d01-2b0d-4a6d-8f11-72279181e674"
      }
    }
  }
}
